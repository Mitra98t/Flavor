let matrix: array(array(int)) = [[1, 2, 3], [4, 5, 6]];
matrix[0][1]++;
matrix[1][2] = matrix[1][0] + matrix[0][2];
let total: int = 0;
let row: int = 0;
while row < 2 {
    let col: int = 0;
    while col < 3 {
        total = total + matrix[row][col];
        col++;
    }
    row++;
}
print "matrix=", matrix, ", total=", total;
